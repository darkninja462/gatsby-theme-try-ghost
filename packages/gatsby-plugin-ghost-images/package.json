{
    "name": "gatsby-plugin-ghost-images",
    "description": "Gatsby plugin that downloads images from Ghost CMS to be used with Gatsby image tools.",
    "version": "1.5.1",
    "license": "MIT",
    "author": "styxlab",
    "homepage": "https://github.com/styxlab/",
    "main": "gatsby-node.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/styxlab/gatsby-theme-try-ghost/tree/master/packages/gatsby-plugin-ghost-images"
    },
    "bugs": {
        "url": "https://github.com/styxlab/gatsby-theme-try-ghost/issues"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "gatsby-image",
        "remote",
        "download",
        "jamify",
        "ghost",
        "ghost-cms"
    ],
    "scripts": {
        "build": "babel src --out-dir . --ignore **/__tests__",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir . --ignore **/__tests__"
    },
    "devDependencies": {
        "@babel/cli": "7.10.4",
        "@babel/core": "7.10.4",
        "babel-preset-gatsby-package": "0.5.1",
        "cross-env": "7.0.2"
    },
    "peerDependencies": {
        "gatsby": "^2.23.20"
    },
    "dependencies": {
        "@babel/runtime": "7.10.4",
        "gatsby-source-filesystem": "^2.3.18",
        "lodash": "^4.17.15"
    }
